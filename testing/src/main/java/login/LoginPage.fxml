<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.PasswordField?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0" fx:controller="login.LoginViewController">
	<!-- TODO Add Nodes -->
	<children>
      <TextField fx:id="usernameField" layoutX="213.0" layoutY="117.0" />
      <Label fx:id="usernameErrorLabel" layoutX="215.0" layoutY="145.0" text="" style="-fx-text-fill: red"/>
      <Label fx:id="userNameLabel" layoutX="131.0" layoutY="122.0" text="Username" />
      <Label fx:id="passwordLabel" layoutX="133.0" layoutY="192.0" text="Password" />
      <PasswordField fx:id="passwordField" layoutX="215.0" layoutY="189.0" />
      <Label fx:id="passwordErrorLabel" layoutX="217.0" layoutY="217.0" text="" style="-fx-text-fill: red"/>
      <Button fx:id="signUp" layoutX="215.0" layoutY="248.0" mnemonicParsing="false" text="Sign Up" onAction="#signUpPage" />
      <Button fx:id="submit" layoutX="315.0" layoutY="248.0" mnemonicParsing="false" text="Submit" onAction="#checkLogin"/>
      <Label fx:id="invalidLoginLabel" layoutX="205.0" layoutY="278.0" text="" style="-fx-text-fill: red"/>
   </children>
</AnchorPane>
